{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": ["off", 4],
    "linebreak-style": ["error", "unix"],
    "quotes": ["warn", "double"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"]
  }
}
